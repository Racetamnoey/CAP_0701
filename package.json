  "private": true,
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/cds-common-content": "^3.0.1",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.11.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": ">=8",
    "rimraf": "^5.0.5",
    "mbt": "^1.2.29"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-project1": "cds watch --open project1/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy test_0701 --delete-services --delete-service-keys --delete-service-brokers",
    "build": "npx rimraf resources mta_archives && npx mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "cds": {
    "requires": {
      "auth": "mocked",
      "db": "hana",
      "[production]": {
        "auth": "xsuaa",
        "db": {
          "kind": "hana",
          "credentials": {}
        }
      },
      "[development]": {
        "auth": "mocked",
        "db": "sqlite"
      }
    },
    "features": {
      "preserve_computed": true
    },
    "log": {
      "service": true
    }
  },
  "sapux": [
    "app/project1"
  ]
}
